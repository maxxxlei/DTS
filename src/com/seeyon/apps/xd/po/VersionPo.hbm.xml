<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.seeyon.apps.xd.po">
    <!-- 版本管理表 -->
  <class
    name="VersionPo"
    table="xd24_version"
  >
    <!-- ID -->
    <id
      name="id"
      type="long"
      column="ID"
      length="21"
    >
      <generator class="assigned"/>
    </id>
    <!-- 版本编号 -->
    <property
      name="vCode"
      column="vCode"
      type="string"
      not-null="false"
      length="255"
    />

    <!-- 年度 -->
    <property
      name="vYear"
      column="v_year"
      type="string"
      not-null="false"
      length="4"
    />
    <!-- 启用状态 -->
    <property
      name="state"
      column="v_state"
      type="integer"
      not-null="false"
    />
    <!-- 创建时间 -->
    <property
      name="createTime"
      column="create_time"
      type="timestamp"
      not-null="false"
    />
    <!-- 修改时间 -->
    <property
      name="updateTime"
      column="update_time"
      type="timestamp"
      not-null="false"
    />
    <!-- 是否删除-->
    <property
      name="isDelete"
      column="is_delete"
      type="integer"
      not-null="false"
    />
    <!-- 描述 -->
    <property
      name="desc"
      column="descr"
      type="string"
      not-null="false"
    />
    <!-- 版本状态，是否启用、停用，
		0：停用
		1：启用 -->
    <property
      name="isEnable"
      column="isEnable"
      type="integer"
      not-null="false"
      length="2"
    />
    <!-- 版本id -->
    <property
      name="versionId"
      column="versionId"
      type="long"
      not-null="false"
      length="21"
    />
  </class>
</hibernate-mapping>
